{
    "code-runner.executorMap": {
        // "typescript": "npx ts-node --files",
        "typescript": "npx ts-node --files --transpile-only",
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll": true
    },
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    }
}
