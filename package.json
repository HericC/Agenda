{
    "name": "agenda",
    "version": "0.0.1",
    "description": "",
    "main": "dist/server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "start": "npm run build:back && node dist/index.js",
        "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build:front": "webpack",
        "build:front-w": "webpack -w",
        "build:back": "tsc",
        "lint": "npx eslint ./src --ext .ts",
        "style:check": "prettier --check src/**/*.ts",
        "style:fix": "prettier --write src/**/*.ts",
        "test": "jest",
        "test:silent": "jest --silent --noStackTrace",
        "test:silent-w": "jest --watchAll --silent --noStackTrace",
        "test:coverage": "jest --coverage --silent --noStackTrace"
    },
    "keywords": [],
    "author": "Heric Macedo Oliveira Silva",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^26.0.24",
        "@types/node": "^16.4.13",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.5",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5",
        "webpack": "^5.49.0",
        "webpack-cli": "^4.7.2"
    }
}
